  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4600",
    "build": "ng build --prod --aot --buildOptimizer --commonChunk --baseHref=/app/  --vendorChunk --optimization --progress && cp .htaccess dist",
    "copy": "rm -r -f ./../backend/public/app/ && cp -r dist/ ./../backend/public/ && mv ./../backend/public/dist ./../backend/public/app",
    "bakend_view": "cp ./../backend/public/app/index.html ./../backend/resources/views/welcome.blade.php && php ./../backend/artisan view:clear",
    "product": "npm run build && npm run copy && npm run bakend_view && service apache2 restart",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
